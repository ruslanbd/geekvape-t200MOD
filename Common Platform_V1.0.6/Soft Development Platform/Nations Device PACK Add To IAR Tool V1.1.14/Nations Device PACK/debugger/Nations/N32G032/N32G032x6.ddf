#include weak "..\..\ARM\ARMv7-M-MPU.ddf"

;; Memory information ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; 
;; Used to define address zones within the ARM address space (Memory). 
;;
;;   Name      may be almost anything
;;   AdrSpace  must be Memory
;;   StartAdr  start of memory block
;;   EndAdr    end of memory block
;;   AccType   type of access, read-only (R), read-write (RW) or SFR (W)

[Memory]
;;         Name             AdrSpace    StartAdr    EndAdr      AccType   Width
Memory =  RemapMem         Memory      0x00000000  0x00007FFF  R 
Memory =  Flash            Memory      0x08000000  0x08007FFF  R 
Memory =  ROM              Memory      0x1FFF0000  0x1FFF0BFF  R 
Memory =  OptionsROM       Memory      0x1FFFF600  0x1FFFF60D  R  
Memory =  RAM              Memory      0x20000000  0x20001FFF  RW
Memory =  PeriphAPB1       Memory      0x40000000  0x4000FFFF  RW 
Memory =  PeriphAPB2       Memory      0x40010000  0x40017FFF  RW 
Memory =  PeriphAHB1       Memory      0x40018000  0x41FFFFFF  RW 
Memory =  Periph1          Memory      0x60000000  0xA0000FFF  RW
Memory =  SystemSFR        Memory      0xE0000000  0xE00FFFFF  W 

TrustedRanges = true
UseSfrFilter = true

[SfrInclude]
File = N32G032.svd
